# Tuskarr Tusks 0 (Used by Skin 0)

## Tusks 0 -  Skin 0
###tuskarrtusks0
pdxmesh = {
	name = "tuskarrtusks0_mesh"
	file = "tuskarrtusks0.mesh"

	meshsettings = {
		name = "tuskarrtusks0Shape"
		index = 0
		texture_diffuse = "tuskarr0.dds"
		texture_normal = "default_yellow_normal.dds"
		texture_specular = "male_elven_eyebrows_0_properties.dds"
		shader = "portrait_attachment_alpha_to_coverage"
		shader_file = "gfx/FX/jomini/portrait.shader"
	}
}
entity = {
	name = "tuskarrtusks0_skin0_entity"
	pdxmesh = "tuskarrtusks0_mesh"
}


