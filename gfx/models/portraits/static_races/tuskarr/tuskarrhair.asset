# Tuskarr Hairstyle 0

###tuskarrhair0_blonde
pdxmesh = {
	name = "tuskarrhair0_mesh"
	file = "tuskarrhair0.mesh"

	meshsettings = {
		name = "tuskarrhair0Shape"
		index = 0
		texture_diffuse = "tuskarrhair.dds"
		texture_normal = "default_blue_normal.dds"
		texture_specular = "male_elven_eyebrows_0_properties.dds"
		shader = "portrait_hair"
		shader_file = "gfx/FX/jomini/portrait.shader"
	}
}
entity = {
	name = "tuskarrhair0_blonde_entity"
	pdxmesh = "tuskarrhair0_mesh"
	
	game_data = {
		portrait_entity_user_data = {
			color_mask_remap_interval = {
				interval = {0.0 1.0}
			}
		}
	}
}

