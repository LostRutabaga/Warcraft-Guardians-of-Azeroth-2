#### mechagnome_stache_a

###mechagnome_stache_a_yellow
pdxmesh = {
	name = "mechagnome_stache_a_mesh"
	file = "mechagnome-stache-a.mesh"

	meshsettings = {
		name = "mechagnome-stache-aShape"
		index = 0
		texture_diffuse = "mechagnome_stache_yellow.dds"
		texture_normal = "default_yellow_normal.dds"
		texture_specular = "male_elven_eyebrows_0_properties.dds"
		shader = "portrait_attachment_alpha_to_coverage"
		shader_file = "gfx/FX/jomini/portrait.shader"
	}
}
