# Mustache Type 1

###pandamustache_1_black
pdxmesh = {
	name = "pandamustache-1_mesh"
	file = "pandamustache-1.mesh"

	meshsettings = {
		name = "pandamustache-1Shape"
		index = 0
		texture_diffuse = "malepandabeard-white.dds"
		texture_normal = "default_blue_normal.dds"
		texture_specular = "male_elven_eyebrows_0_properties.dds"
		shader = "portrait_hair"
		shader_file = "gfx/FX/jomini/portrait.shader"
	}
}
entity = {
	name = "pandamustache_1_black_entity"
	pdxmesh = "pandamustache-1_mesh"
	
	game_data = {
		portrait_entity_user_data = {
			color_mask_remap_interval = {
				interval = {0.0 1.0}
			}
		}
	}
}

